{
  "entrypoint": "index.html",
  "shell": "src/components/bnb-app.js",
  "fragments": [
    "src/components/bnb-add-page.js",
    "src/components/bnb-bytes-details.js",
    "src/components/bnb-edit-page.js",
    "src/components/bnb-edit-password.js",
    "src/components/bnb-forgot-password.js",
    "src/components/bnb-home.js",
    "src/components/bnb-lighthouse-details.js",
    "src/components/bnb-members.js",
    "src/components/bnb-page.js",
    "src/components/bnb-performance-details.js",
    "src/components/bnb-requests-details.js",
    "src/components/bnb-signin.js",
    "src/components/bnb-signup.js",
    "src/components/bnb-uptime-details.js",
    "src/components/bnb-user-preferences.js",
    "src/components/lazy-resources.js"
  ],
  "sources": [
    "src/**/*",
    "images/**/*"
  ],
  "extraDependencies": [
    "manifest.json",
    "push-handler.js",
    "node_modules/@webcomponents/webcomponentsjs/**"
  ],
  "builds": [
    {
      "name": "esm-bundled",
      "browserCapabilities": [
        "es2015",
        "modules"
      ],
      "js": {
        "minify": { "minify": {"exclude": ["*.min.js"]}}
      },
      "css": {
        "minify": true
      },
      "html": {
        "minify": true
      },
      "bundle": true,
      "addServiceWorker": true
    },
    {
      "name": "es6-bundled",
      "browserCapabilities": [
        "es2015"
      ],
      "js": {
        "compile": "es2015",
        "minify": { "minify": {"exclude": ["*.min.js"]}},
        "transformModulesToAmd": true
      },
      "css": {
        "minify": true
      },
      "html": {
        "minify": true
      },
      "bundle": true,
      "addServiceWorker": true
    }
  ],
  "moduleResolution": "node",
  "npm": true,
  "lint": {
    "rules": [
      "polymer-3"
    ]
  }
}
